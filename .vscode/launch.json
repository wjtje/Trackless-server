  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "dev"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to node",
      "type": "node",
      "request": "attach",
      "restart": true,
      "port": 9229,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "localRoot": "${workspaceFolder}\\dist",
      "remoteRoot": "C:\\Users\\wjtje\\Local Storage SSD\\projects\\Trackless\\server\\dist"
    }
  ]
